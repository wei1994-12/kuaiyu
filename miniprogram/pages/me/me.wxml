<!--pages/me/me.wxml-->
<view class="cover">
  <van-image round
    width="160rpx"
    height="160rpx"
    src="{{avatarUrl}}" 
    bindtap="tapImage"/>
  <view></view>
  <text bindtap="tapLogin">{{nickName}}</text>
</view>
<van-cell-group>
  <van-cell title="我的订单" icon="location-o" is-link />
  <van-cell title="优惠券" icon="coupon-o" is-link />
  <van-cell title="影院会员卡" icon="location-o" is-link />
</van-cell-group>

<van-cell-group>
  <van-cell title="想看的电影" icon="location-o" is-link />
  <van-cell title="看过的电影" icon="location-o" is-link />
</van-cell-group>














